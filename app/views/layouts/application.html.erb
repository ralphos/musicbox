<!DOCTYPE html>
<html>
<head>
  <title>Musicbox</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="flash">
  <ul>
    <% flash.each do |key, msg| %>
    <%= content_tag :li, msg, id: key %>
    <% end %>
  </ul>
</div>

<div class="hmenu">
  <%= render 'shared/navigation' %>
</div>

<%= yield %>

</body>
</html>
